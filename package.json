{
  "name": "poc-jss",
  "description": "Application template for Sitecore JSS and React.",
  "version": "0.0.1",
  "private": true,
  "config": {
    "appName": "poc-jss",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/poc-jss",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "language": "en",
    "tunnelUrl": "http://jss.ngrok.io",
    "graphQLEndpointPath": "/api/poc-jss"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "author": {
    "name": "DEG",
    "url": "https://www.degdigital.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/degdigital/sitecore-jss-app-template-react.git"
  },
  "bugs": {
    "url": "https://github.com/degdigital/sitecore-jss-app-template-react/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-jss/sitecore-jss-react": "^13.0.0",
    "@sitecore-jss/sitecore-jss-tracking": "^13.0.0",
    "axios": "~0.19.0",
    "classnames": "^2.2.6",
    "coveoforsitecore-jss": "0.0.12",
    "globalize": "^0.1.1",
    "google-maps-react": "^2.0.2",
    "prop-types": "15.7.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-helmet": "~5.2.1",
    "react-router-dom": "~5.1.0",
    "serialize-javascript": "~2.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "~7.6.2",
    "@sitecore-jss/sitecore-jss-cli": "^11.0.2",
    "@sitecore-jss/sitecore-jss-dev-tools": "^11.0.2",
    "@sitecore-jss/sitecore-jss-manifest": "^11.0.2",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addon-viewport": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "~9.0.2",
    "chalk": "~2.4.2",
    "chokidar": "~3.1.1",
    "cross-env": "~6.0.0",
    "css-loader": "^3.3.0",
    "file-loader": "^5.0.2",
    "html-loader": "~0.5.5",
    "http-proxy-middleware": "~0.20.0",
    "mini-css-extract-plugin": "^0.9.0",
    "ngrok": "^3.2.5",
    "npm-run-all": "~4.1.5",
    "null-loader": "~3.0.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.1",
    "speed-measure-webpack-plugin": "^1.3.1",
    "stats-webpack-plugin": "^0.7.0",
    "url-loader": "~2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "~3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "build:rendering-host": "npm-run-all --serial bootstrap:connected build:client:rendering-host build:server",
    "build": "npm-run-all --serial bootstrap:connected build:client build:server",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"webpack --config webpack.prod.js\"",
    "build:client:rendering-host": "cross-env-shell PUBLIC_URL=$npm_package_config_tunnelUrl \"webpack --config webpack.prod.js\"",
    "build:server": "cross-env-shell NODE_ENV=production \"webpack --config server/server.webpack.config.js\"",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "build-storybook": "build-storybook -s public",
    "jss": "jss",
    "scaffold": "node scripts/scaffold-component.js",
    "start": "npm-run-all --serial bootstrap:disconnected --parallel start:react start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components",
    "start:connected-ssr": "scjss-verify-setup && npm-run-all --serial bootstrap:connected --parallel start:react start:watch-components start:http-renderer",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:react": "webpack-dev-server --open --config webpack.dev.js",
    "start:rendering-host": "cross-env-shell NODE_ENV=development JSS_RENDER_ENGINE=http \"node scripts/http-renderer.js\"",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "storybook": "start-storybook -p 9009 -s public"
  }
}